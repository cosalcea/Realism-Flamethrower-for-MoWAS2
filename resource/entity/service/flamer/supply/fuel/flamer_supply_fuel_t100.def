{game_entity
	{extension "/properties/piece.ext"}
	{simulation {able 1}}
	{constrain none}
	{obstacleID "nothing"}
	{collider "equipbox"}
	{Volume "body"
		{able clear {contact}{contact_ground}{touch}}
		{density 1}
	}
	{Volume "select"
		{density 0}
	}
	{body
		{proper body}
		{permit body}
	}

	{extender "inventory"
		{box
			{weight 100}
			{size 4 5}
	        {item "balloon_j" filled}
		}
	}
	{extender "stuff"
		{item "dummy_supply"} ; allow game engine to delete it when too many stuffs exist.
	}

	{Extension "/entity/inventory/box2/box2.mdl"}

	{props "flamer_supply_box" "steel"}
	{props "nocover"}
}
